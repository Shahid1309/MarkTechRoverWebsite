{
  "name": "marktechrover-website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "MarkTechRover - Leading Digital Growth & Marketing Agency Website",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run generate-sitemap && npm run generate-rss && npm run generate-feeds",
    "build": "vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "performance": "lighthouse http://localhost:5174 --output=json --output-path=./lighthouse-report.json",
    "bundle-analyzer": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "generate-sitemap": "node scripts/generate-dynamic-sitemap.js",
    "generate-rss": "node src/scripts/generateRSS.js",
    "seo-audit": "node src/scripts/seoAudit.js",
    "generate-feeds": "node src/scripts/generateFeed.js",
    "optimize-images": "vite-plugin-image-optimizer",
    "move-portfolio": "node scripts/move-portfolio-images.js",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules/.vite .cache",
    "dev:clean": "npm run clean && npm run dev",
    "build:clean": "npm run clean && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.4",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "^2.15.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-reconciler": "^0.29.0",
    "react-router-dom": "^6.22.3",
    "scheduler": "^0.23.0",
    "sitemap": "^8.0.0",
    "svgo": "^4.0.0",
    "tailwind-merge": "^2.2.1",
    "three": "^0.159.0",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet-async": "^1.0.3",
    "@types/three": "^0.159.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.2",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-imagetools": "^7.1.0",
    "vite-plugin-image-optimizer": "^1.1.8"
  }
}
